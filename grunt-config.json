{
  "autoprefixerBrowsers": [
    "Android 2.3",
    "Android >= 4",
    "Chrome >= 20",
    "Firefox >= 24",
    "Explorer >= 8",
    "iOS >= 6",
    "Opera >= 12",
    "Safari >= 6"
  ],
  "paths": {
    "bootstrapSrc": "packages/bootstrap/",
    "dest": {
      "bootstrap": "dist/assets/js/lib/bootstrap/",
      "build": "dist/assets/",
      "css": "dist/assets/css/",
      "dist": "dist/",
      "font": "dist/assets/font/",
      "fonts": "dist/assets/fonts/",
      "greensock": "dist/assets/js/lib/greensock/",
      "js": "dist/assets/js/",
      "lib": "dist/assets/js/lib/"
    },
    "src": {
      "html": "src/",
      "js": "src/js/",
      "less": "src/less/"
    },
    "pkg": "bower_components/",
    "manualPkg": "packages/"
  }
}
